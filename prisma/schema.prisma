generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model G84_bike_net_pt {
  id         Int                      @id @default(autoincrement())
  geom       Unsupported("geometry")?
  objectid   String?                  @db.VarChar(254)
  cnn        BigInt?
  corner     String?                  @db.VarChar(254)
  street1    String?                  @db.VarChar(254)
  street2    String?                  @db.VarChar(254)
  descript   String?                  @db.VarChar(254)
  count      BigInt?
  install_mo String?                  @db.VarChar(254)
  install_yr BigInt?
  update_mo  String?                  @db.VarChar(254)
  update_yr  BigInt?
  sf_find_ne BigInt?                  @map("sf find ne")
  current_po BigInt?                  @map("current po")
  current_su BigInt?                  @map("current su")
  analysis_n BigInt?                  @map("analysis n")
  neighborho BigInt?
}

model SFMTA_Bikeway_Network_Point_Features {
  id                             Int     @id @default(autoincrement())
  OBJECTID                       String? @db.VarChar
  CNN                            String? @db.VarChar
  CORNER                         String? @db.VarChar
  STREET1                        String? @db.VarChar
  STREET2                        String? @db.VarChar
  DESCRIPT                       String? @db.VarChar
  COUNT                          String? @db.VarChar
  INSTALL_MO                     String? @db.VarChar
  INSTALL_YR                     String? @db.VarChar
  UPDATE_MO                      String? @db.VarChar
  UPDATE_YR                      String? @db.VarChar
  shape                          String? @db.VarChar
  SF_Find_Neighborhoods_2        String? @map("SF Find Neighborhoods 2") @db.VarChar
  Current_Police_Districts_2     String? @map("Current Police Districts 2") @db.VarChar
  Current_Supervisor_Districts_2 String? @map("Current Supervisor Districts 2") @db.VarChar
  Analysis_Neighborhoods_2       String? @map("Analysis Neighborhoods 2") @db.VarChar
  Neighborhoods                  String? @db.VarChar
}

model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)
}

model G84_st_ctl_li {
  id         Int                      @id @default(autoincrement())
  geom       Unsupported("geometry")?
  fid        Int?
  jurisdicti String?                  @db.VarChar
  zip_code   String?                  @db.VarChar
  f_node_cnn String?                  @db.VarChar
  date_dropp String?                  @db.VarChar
  gds_chg_id String?                  @db.VarChar
  classcode  String?                  @db.VarChar
  t_st       String?                  @db.VarChar
  accepted   String?                  @db.VarChar
  street_gc  String?                  @db.VarChar
  cnn        String?                  @db.VarChar
  layer      String?                  @db.VarChar
  nhood      String?                  @db.VarChar
  st_type    String?                  @db.VarChar
  streetname String?                  @db.VarChar
  rt_fadd    String?                  @db.VarChar
  f_st       String?                  @db.VarChar
  streetna_1 String?                  @db.VarChar
  lf_fadd    String?                  @db.VarChar
  date_alter String?                  @db.VarChar
  t_node_cnn String?                  @db.VarChar
  date_added String?                  @db.VarChar
  street     String?                  @db.VarChar
  gds_chg__1 String?                  @db.VarChar
  lf_toadd   String?                  @db.VarChar
  gds_chg__2 String?                  @db.VarChar
  oneway     String?                  @db.VarChar
  rt_toadd   String?                  @db.VarChar
  active     String?                  @db.VarChar
  ASSIGNEE   String?                  @db.VarChar

  @@index([geom], map: "sidx_G84_st_ctl_li_geom", type: Gist)
}

model bikeway2_pt {
  id                             Int                      @id @default(autoincrement())
  geom                           Unsupported("geometry")?
  fid                            BigInt?
  OBJECTID                       Int?
  CNN                            Int?
  CORNER                         String?                  @db.VarChar
  STREET1                        String?                  @db.VarChar
  STREET2                        String?                  @db.VarChar
  DESCRIPT                       String?                  @db.VarChar
  COUNT                          Int?
  INSTALL_MO                     Int?
  INSTALL_YR                     Int?
  UPDATE_MO                      Int?
  UPDATE_YR                      Int?
  SF_Find_Neighborhoods_2        Int?                     @map("SF Find Neighborhoods 2")
  Current_Police_Districts_2     Int?                     @map("Current Police Districts 2")
  Current_Supervisor_Districts_2 Int?                     @map("Current Supervisor Districts 2")
  Analysis_Neighborhoods_2       Int?                     @map("Analysis Neighborhoods 2")
  Neighborhoods                  Int?
}

model mta_bikeway_pt {
  id                             Int                      @id @default(autoincrement())
  geom                           Unsupported("geometry")?
  fid                            BigInt?
  OBJECTID                       Int?
  CNN                            Int?
  CORNER                         String?                  @db.VarChar
  STREET1                        String?                  @db.VarChar
  STREET2                        String?                  @db.VarChar
  DESCRIPT                       String?                  @db.VarChar
  COUNT                          Int?
  INSTALL_MO                     Int?
  INSTALL_YR                     Int?
  UPDATE_MO                      Int?
  UPDATE_YR                      Int?
  SF_Find_Neighborhoods_2        Int?                     @map("SF Find Neighborhoods 2")
  Current_Police_Districts_2     Int?                     @map("Current Police Districts 2")
  Current_Supervisor_Districts_2 Int?                     @map("Current Supervisor Districts 2")
  Analysis_Neighborhoods_2       Int?                     @map("Analysis Neighborhoods 2")
  Neighborhoods                  Int?
}
